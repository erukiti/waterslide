{
    "parser": "babel-eslint",
    "rules": {
        "no-alert": 1,
        "no-eval": 1,
        "no-global-assign": 2,
        "no-implicit-coercion": 1,
        "no-implicit-globals": 1,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 1,
        "no-loop-func": 2,
        "no-multi-spaces": 1,
        "no-multi-str": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal-escape": 1,
        "no-param-reassign": 1,
        "no-proto": 1,
        "no-return-assign": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-throw-literal": 1,
        "no-unmodified-loop-condition": 1,
        "no-useless-call": 1,
        "no-useless-escape": 1,
        "no-void": 1,
        "no-script-url": 1,
        "no-warning-comments": 1,
        "no-with": 2,
        "no-catch-shadow": 1,
        "no-label-var": 1,
        "no-undefined": 1,
        "handle-callback-err": 1,
        "no-new-require": 1,
        "array-bracket-spacing": [1, "never"],
        "block-spacing": [1, "never"],
        "brace-style": [1, "1tbs"],
        "camelcase": 1,
        "comma-spacing": 1,
        "comma-style": 1,
        "computed-property-spacing": 1,
        "func-call-spacing": 1,
        "indent": [1, 4],
        "key-spacing": 1,
        "keyword-spacing": 1,
        "eol-last": 1,
        "radix": 1,
        "yoda": 1,
        "max-depth": 1,
        "new-cap": 1,
        "new-parens": 1,
        "no-array-constructor": 1,
        "no-mixed-operators": 1,
        "no-multi-assign": 1,
        "no-multiple-empty-lines": 1,
        "no-nested-ternary": 1,
        "no-new-object": 1,
        "no-tabs": 1,
        "no-trailing-spaces": 1,
        "prefer-promise-reject-errors": 1,
        "semi": [1, "never"],
        "quotes": [2, "single"],
        "linebreak-style": [1,"unix"],
        "no-console": 0,
        "arrow-spacing": 1,
        "generator-star-spacing": 1,
        "no-useless-constructor": 1,
        "no-var": 2,
        "no-unused-vars": 0,
        "no-unneeded-ternary": 1,
        "no-whitespace-before-property": 1,
        "nonblock-statement-body-position": 2,
        "object-curly-spacing": 1,
        "one-var": [1, "never"],
        "space-before-blocks": 1,
        "space-before-function-paren": [1, "never"],
        "space-in-parens": 1,
        "space-infix-ops": 1,
        "object-shorthand": 1,
        "no-confusing-arrow": 2,
        "no-useless-computed-key": 1,
        "no-duplicate-imports": 1,
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-rest-params": 1,
        "prefer-spread": 1,
        "prefer-template": 1,
        "rest-spread-spacing": [1, "never"],
        "require-yield": 1,
        "template-curly-spacing": 1,
        "yield-star-spacing": 1,
        "complexity": [1, 20],
        "eqeqeq": 2,
        "no-shadow": 1
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended"
}
